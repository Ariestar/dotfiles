{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "╭─"
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#a6e22e",
          "template": "{{ .UserName }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "<#75715e>at</> "
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#66d9ef",
          "template": "{{ .HostName }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "<#75715e>in</> "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#f92672",
          "options": {
            "style": "full"
          },
          "template": "<b>{{ .Path }}</b> "
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "{{ .CurrentDate | date \"06-01-02\" }} <#75715e>-</> {{ .CurrentDate | date \"15:04:05\" }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "╰─○ "
        }
      ]
    }
  ],
  "final_space": true
}